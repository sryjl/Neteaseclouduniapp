.input-box-all {
  width: 100%;
  height: 88rpx;
  padding: 0 40rpx;
  border-bottom: 1px solid #F5F5F5;
}
.input-box {
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  color: #4A4A4A;
}
.input-box-left {
  margin-right: 20rpx;
}
.input-box-center {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.input-box-clear {
  margin-left: 10rpx;
  font-size: 28rpx;
}
.input-box-right {
  margin-left: 10rpx;
}
.input-box .password-control {
  margin-left: 10rpx;
  width: 36rpx;
  height: 36rpx;
}
