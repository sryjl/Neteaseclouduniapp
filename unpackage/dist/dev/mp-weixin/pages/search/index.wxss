.firesearch {
  height: 60rpx;
  border-bottom: 1px solid #E6E6E6;
  text-align: left;
}
.firesearch text {
  margin: 0;
  height: 40rpx !important;
  font-size: 30rpx;
  vertical-align: middle;
}
.firesearch .fire {
  margin-left: 30rpx;
  height: 30rpx;
  width: 30rpx;
  vertical-align: middle;
}
.hotlist::after {
  content: "";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.lefthot {
  width: 46%;
  float: left;
  margin-top: 12rpx;
  margin-left: 4%;
}
.lefthot navigator {
  font-size: 30rpx;
  margin-bottom: 20rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.lefthot navigator text {
  margin-right: 15rpx;
}
.lefthot navigator .lessthanten {
  margin-right: 30rpx !important;
}
.righthot {
  width: 50%;
  float: left;
  margin-top: 12rpx;
}
.righthot navigator {
  font-size: 30rpx;
  margin-bottom: 20rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.righthot navigator text {
  margin-right: 15rpx;
}
.righthot navigator .lessthanten {
  margin-right: 30rpx !important;
}
.hotserchmore {
  margin-top: 30rpx;
  text-align: center;
  font-size: 20rpx;
  color: #808080;
}
.hide {
  display: none;
}
.bold {
  font-weight: bold;
}
.bold text {
  color: #EC4141;
}

